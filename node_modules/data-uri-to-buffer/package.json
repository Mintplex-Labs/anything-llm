  "bugs": {
    "url": "https://github.com/TooTallNate/node-data-uri-to-buffer/issues"
  },
  "homepage": "https://github.com/TooTallNate/node-data-uri-to-buffer",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^12.20.36",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
