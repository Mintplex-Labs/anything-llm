{
  "3.2.1 Variable Expansion" :
  {
    "variables": {
       "count"      : ["one", "two", "three"],
       "dom"        : ["example", "com"],
       "dub"        : "me/too",
       "hello"      : "Hello World!",
       "half"       : "50%",
       "var"        : "value",
       "who"        : "fred",
       "base"       : "http://example.com/home/",
       "path"       : "/foo/bar",
       "list"       : ["red", "green", "blue"],
       "keys"       : { "semi" : ";", "dot" : ".", "comma" : ","},
       "v"          : "6",
       "x"          : "1024",
       "y"          : "768",
       "empty"      : "",
       "empty_keys" : [],
       "undef"      : null
     },
     "testcases" : [
        ["{count}", "one,two,three"],
        ["{count*}", "one,two,three"],
        ["{/count}", "/one,two,three"],
        ["{/count*}", "/one/two/three"],
        ["{;count}", ";count=one,two,three"],
        ["{;count*}", ";count=one;count=two;count=three"],
        ["{?count}", "?count=one,two,three"],
        ["{?count*}", "?count=one&count=two&count=three"],
        ["{&count*}", "&count=one&count=two&count=three"]
      ]
  },
